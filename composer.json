{
    "name": "gillix/gillix",
    "description": "Gillix project skeleton",
    "type": "project",
    "license": "MIT",
    "require": {
        "gillix/framework": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php bin/compile"
        ]
    }
}
